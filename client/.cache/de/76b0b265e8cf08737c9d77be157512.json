    <div class=\"field\">\n                <div class=\"ui left icon input\">\n                  <i class=\"lock icon\"></i>\n                  <input\n                    type=\"password\"\n                    name=\"password\"\n                    placeholder=\"Password\"\n                    v-model=\"registerForm.password\"\n                  />\n                </div>\n              </div>\n              <input class=\"ui fluid large blue submit button\" type=\"submit\" value=\"Sign Up\">\n            </div>\n\n            <div class=\"ui error message\"></div>\n          </form>\n          <div class=\"ui message\">\n            Already have account? <a href=\"#\" id=\"sign-in-show\" @click.prevent=\"toLogin\">Sign in</a>\n          </div>\n    </div> \n</template>\n\n<script>\nexport default {\n    name: 'Register',\n    props:['page'],\n    data(){\n        return{\n            registerForm: {\n                name: '',\n                email: '',\n                password: ''\n            }\n        }\n    }, \n    methods: {\n        register(){\n          console.log('register')\n            axios({\n                url: `http://localhost:3000/users/register`,\n                method: 'POST',\n                data: {\n                    name: this.registerForm.name,\n                    email: this.registerForm.email,\n                    password: this.registerForm.password\n                }\n            })\n            .then(({data}) => {\n              localStorage.setItem('token', data.accesstoken)\n              this.$emit('toLogin', 'MainPage')\n            })\n            .catch((err)=> {\n                console.log(err)\n            })\n        },\n        toLogin(){\n          this.$emit('toLogin', 'loginForm')\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>\n"},"lineCount":null}},"error":null,"hash":"8b425caab63e8cde94479585ba30cae1","cacheData":{"env":{}}}